{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{controller}/{endpointmethod}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5000
        }
      ],
      "ServiceName": "api-databuilder-service",
      "LoadBalancerOptions": {
        "Type": "LeastConnection"
      },
      "UpstreamPathTemplate": "/core/{controller}/{endpointmethod}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put" ]
    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Namespace": "default",
      "Type": "kube",
      "Port": 5000
    }
  }
}